<li>{{this.name}}- {{this.artist}}</li>
{{/each}}
</ul> --}}

<table>
<thead>
    <th>Name</th>
    <th>Year</th>
    <th>Artist</th>
    <th>Album</th>
    <th>Features</th>
 </thead>
 <tbody>
    {{#each songs}}
    <tr>
      <td>{{this.name}}</td>
      <td>{{this.year}}</td>
      <td>{{this.artist}}</td>
      <td>{{this.album}}</td>
      <td>{{this.features}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>